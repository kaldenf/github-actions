  "lint-staged": {
    "src/App/**/*.{js,ts,tsx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "preset": "react-native",
    "globals": {
      "__UNIT_TEST__": true
    },
    "testRegex": "dist/src/test/unit/.+\\.test\\.(js|ts|tsx)$",
    "moduleFileExtensions": [
      "js",
      "json",
      "es6",
      "ios.js",
      "android.js",
      "ts",
      "tsx"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/dist/src/test/helpers/testSetup.js",
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/scripts/jest/assetsTransformer.js",
      "react-navigation-props-mapper": "<rootDir>/scripts/jest/withMappedNavigationParamsTransformer.js",
      "^React$": "<rootDir>/node_modules/react",
      "^react-native$": "<rootDir>/node_modules/react-native"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|@react-navigation|@react-native-community|rn-material-ui-textfield|@taskrabbit)"
    ],
    "snapshotResolver": "<rootDir>/scripts/jest/snapshotResolver.js",
    "timers": "fake"
  },
