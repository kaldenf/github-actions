{
  "name": "tasker",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start:all": "concurrently --kill-others 'yarn start' 'yarn ts-watch --preserveWatchOutput'",
    "start:all!": "concurrently --kill-others 'yarn start --reset-cache' 'yarn ts-watch --preserveWatchOutput'",
    "start": "react-native start",
    "reset": "yarn clean:dist && yarn start:all",
    "reset!": "yarn clean:all && yarn install && yarn start:all!",
    "postinstall": "sh scripts/post_dependency_install.sh",
    "precommit": "lint-staged",
    "ts-watch": "tsc-watch --onSuccess 'sh scripts/on_tsc_success.sh'",
    "compile:ts": "yarn tsc && sh scripts/on_tsc_success.sh",
    "clean:all": "watchman watch-del-all; yarn clean:tmp; yarn clean:node-modules; yarn clean:dist; yarn clean:pods",
    "clean:tmp": "rm -rf $TMPDIR/react-*; rm -rf $TMPDIR/metro-*",
    "clean:dist": "rm -rf dist",
    "clean:node-modules": "rm -rf node_modules",
    "clean:android": "cd android; ./gradlew clean; cd ../",
    "clean:pods": "rm -rf ios/Podfile.lock; rm -rf ios/Pods",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet",
    "jest": "TZ=America/Los_Angeles jest --detectOpenHandles --colors",
    "jest:debug": "echo \"\\x1b[30m\\x1b[103m***Attention*** Open chrome://inspect in Chrome to inspect breakpoint.\\x1b[0m\"; node --inspect-brk ./node_modules/.bin/jest --runInBand --testPathPattern",
    "jest:update:snapshots": "TZ=America/Los_Angeles jest --detectOpenHandles -u",
    "android:detox": "detox build -c android.remote && detox test -c android.remote # INDENDED FOR CI",
    "android:detox:build": "detox build -c android.remote # INDENDED FOR CI",
    "android:detox:test": "detox test -c android.remote # INDENDED FOR CI",
    "ios:detox": "detox build -c ios.remote && detox test -c ios.remote # INTENDED FOR CI",
    "ios:detox:build_framework": "detox build-framework-cache # INTENDED FOR CI",
    "ios:detox:build": "detox build -c ios.remote # INTENDED FOR CI",
    "ios:detox:test": "detox test -c ios.remote --record-videos failing",
    "arm:pods": "cd ios && arch -x86_64 pod install && cd ../",
    "android": "cd android && ./gradlew installDebugstaging; cd ../ # POINTS TO STAGING SERVER",
    "android:clean": "cd android; ./gradlew clean; cd ../",
    "android:debug": "cd android && ./gradlew installDebug; cd ../ # POINTS TO LOCAL SERVER",
    "android:shake": "adb shell input keyevent 82",
    "android:tcp": "adb reverse tcp:8081 tcp:8081",
    "compile:ios:test": "PLATFORM=ios TARGET=test node tasks/compile.js",
    "compile:ios:remote_test": "PLATFORM=ios TARGET=remote_test USE_CACHED_COMPILATION=1 node tasks/compile.js",
    "compile:ios:staging": "PLATFORM=ios TARGET=staging node tasks/compile.js",
    "install:ios:staging": "PLATFORM=ios PHONE=true TARGET=staging node tasks/compile.js",
    "compile:android:test": "PLATFORM=android TARGET=test node tasks/compile.js",
    "compile:android:remote_test": "PLATFORM=android TARGET=remote_test node tasks/compile.js",
    "compile:android:staging": "PLATFORM=android TARGET=staging node tasks/compile.js",
    "install:android:staging": "PLATFORM=android PHONE=true TARGET=staging node tasks/compile.js",
    "compile:android:release": "PLATFORM=android TARGET=release node tasks/compile.js",
    "install:android:release": "PLATFORM=android PHONE=true TARGET=release node tasks/compile.js",
    "codemagic:buildCurrentBranch": "ACTION=buildCurrentBranch ts-node ./tasks/codemagic.ts",
    "codemagic:buildRelease": "ACTION=buildRelease ts-node ./tasks/codemagic.ts",
    "reactotron": "sh scripts/reactotron.sh",
    "translation:dump": "ACTION=dump node tasks/translation.js",
    "translation:syncNative": "ACTION=syncNative node tasks/translation.js",
    "translation:backfill": "ACTION=backfill node tasks/translation.js",
    "translation:datetimes": "ACTION=datetimes node tasks/translation.js",
    "translation:check": "ACTION=check node tasks/translation.js",
    "autocommit": "node tasks/auto-commit.js",
    "autocommit:syncNative": "ACTION=syncNative node tasks/auto-commit.js",
    "graphql:codegen": "yarn graphql-code-generator --config ./codegen.yml"
  },
